{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.tslint": true,
    "source.organizeImports": true
  },
  "javascript.validate.enable": false,
  "typescript.validate.enable": false,

  "editor.tokenColorCustomizations": {
    "[Default Dark+]": {
      "functions": "#1ED760",
      "keywords": "#f92672",
      "strings": "#FFD866",
      "comments": "#5f5f5f",
      "numbers": "#c74ded",
      //"variables": "#0af7d3",
      //"variables": "#ff79cd",
      "variables": "#93E6FC",

      "textMateRules": [
        {
          "scope": "variable.other.constant",
          "settings": {
            "foreground": "#c74ded"
          }
        },
        {
          "scope": [
            "keyword.operator",
            "keyword.control",
            "keyword.operator.new"
          ],
          "settings": {
            "foreground": "#f92672"
          }
        },

        {
          "scope": "entity.name.tag",
          "settings": {
            //"foreground": "#1c99e6"
            "foreground": "#096bff"
            //"foreground": "#1c99e6b4"
          }
        },
        {
          "scope": "variable.parameter",
          "settings": {
            //"foreground": "#CE9178"
            "foreground": "#FC9867"
            //"foreground": "#ffb86c"
          }
        },
        {
          "scope": "entity.other.attribute-name",
          "settings": {
            //"foreground": "#e74c3c",
            //"foreground": "#00e8c6"
            "foreground": "#96E072"
          }
        }
      ]
    }
  },
  "peacock.color": "#3c873a",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#4cab49",
    "activityBar.activeBorder": "#4c4fb3",
    "activityBar.background": "#4cab49",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#4c4fb3",
    "activityBarBadge.foreground": "#e7e7e7",
    "sash.hoverBorder": "#4cab49",
    "statusBar.background": "#3c873a",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#4cab49",
    "statusBarItem.remoteBackground": "#3c873a",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#3c873a",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#3c873a99",
    "titleBar.inactiveForeground": "#e7e7e799"
  }
}
